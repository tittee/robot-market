    "baseUrl": "./src",
    "paths": {
      "assets/*": [
        "./assets/*"
      ],
      "components/*": [
        "./components/*"
      ],
      "config/*": [
        "./config/*"
      ],
      "pages/*": [
        "./pages/*"
      ],
      "redux/*": [
        "./redux/*"
      ],
      "router/*": [
        "./router/*"
      ],
      "style/*": [
        "./style/*"
